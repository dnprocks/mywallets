  roe                    String?  @db.VarChar()
  ev_ebitda              String?  @db.VarChar()
  liquidez_corr          String?  @db.VarChar()
  ev_ebit                String?  @db.VarChar()
  div_br_patrim          String?  @db.VarChar()
  cres_rec_5a            String?  @db.VarChar()
