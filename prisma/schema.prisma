  p_ativos               String?  @db.VarChar()
  marg_liquida           String?  @db.VarChar()
  p_cap_giro             String?  @db.VarChar()
  ebit_ativo             String?  @db.VarChar()
  p_ativ_circ_liq        String?  @db.VarChar()
  roic                   String?  @db.VarChar()
